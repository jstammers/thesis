{
    "latex-workshop.latex.recipes": [{
        "name": "latexmk",
        "tools": [
          "latexmk"
        ]
      }],
    "latex-workshop.latex.tools":[
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "--shell-escape",
                "-pdf","-f",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.latex.clean.enabled": true,
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.view.pdf.hand": true,
    "cSpell.enabled": true,
    "cSpell.ignoreRegExpList": [
        "\\\\\\w*(\\[.*?\\])?(\\{.*?\\})?",
        "\\$.+?\\$"
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.words": [
        "Acousto",
        "Aoms",
        "Ecdls",
        "Edfas",
        "Electro",
        "FPGA",
        "HSDIO",
        "Muquans",
        "Niobate",
        "PPLN",
        "Quans",
        "Raman",
        "Refs",
        "TTls",
        "ifpackageloaded",
        "linkcolor",
        "pdfauthor",
        "pdfkeywords",
        "pdfsubject",
        "pdftitle",
        "repump",
        "setup"
    ]
}